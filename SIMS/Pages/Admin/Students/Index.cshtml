@page
@model SIMS.Pages.Admin.Students.IndexModel
@{
    Layout = "~/Pages/Shared/_AdminLayout.cshtml";
    ViewData["Title"] = "Quản lý sinh viên";
}
<!-- Page Heading -->
<h1 class="h3 mb-2 text-gray-800">Danh sách sinh viên</h1>

<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Danh sách sinh viên</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>STT</th>
                        <th>Mã Sinh Viên</th>
                        <th>Số Sinh Viên</th>
                        <th>Họ Và Tên</th>
                        <th>URL</th>
                        <th>Ngày Sinh</th>
                        <th>Email</th>
                        <th>#</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Tiger Nixon</td>
                        <td>System Architect</td>
                        <td>Edinburgh</td>
                        <td>61</td>
                        <td>2011/04/25</td>
                        <td>$320,800</td>
                        <td>$320,800</td>
                        <td>
                            <a class="anticon anticon-edit btn btn-warning btn-tone m-r-5" asp-action="Edit">
                                <i class="fas fa-edit"></i>
                            </a>
                            <a class="anticon anticon-delete btn btn-danger btn-tone m-r-5" asp-action="Delete">
                                <i class="fas fa-trash-alt"></i>
                            </a>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>